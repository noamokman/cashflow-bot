{
  "name": "cashflow-bot",
  "version": "0.4.11",
  "description": "Cashflow bot",
  "exports": "./dist/index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint src --fix --ext .ts",
    "test": "npm run lint && npm run type-check",
    "start": "babel-node src/index.ts --extensions \".ts\"",
    "start:prod": "node dist/index.js",
    "dev": "nodemon",
    "type-check": "tsc --noEmit",
    "build": "rimraf dist && npm run build:js",
    "build:js": "babel src --out-dir dist --extensions \".ts\"",
    "prepare": "husky install",
    "format": "prettier --write '**/*.{ts,js,json}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noamokman/cashflow-bot.git"
  },
  "keywords": [],
  "author": "Noam Okman <noamokman@gmail.com> (https://github.com/noamokman)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noamokman/cashflow-bot/issues"
  },
  "homepage": "https://github.com/noamokman/cashflow-bot#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*.{ts,js,json}": "prettier --write"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/node": "^7.16.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@osskit/eslint-config": "^0.0.8",
    "@osskit/prettier-config": "^0.0.1",
    "@types/lodash-es": "^4.17.5",
    "@types/lowdb": "^1.0.11",
    "@types/node": "^17.0.13",
    "@types/pino": "^7.0.5",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.7.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.2",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "dotenv": "^14.3.2",
    "env-var": "^7.1.1",
    "israeli-bank-scrapers": "^1.10.3",
    "lodash-es": "^4.17.21",
    "lowdb": "^3.0.0",
    "pino": "^7.6.4",
    "read-pkg-up": "^9.0.0",
    "telegraf": "^4.6.0",
    "telegraf-inline-menu": "^6.3.0",
    "telegraf-stateless-question": "^2.0.1"
  }
}
