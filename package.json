{
  "name": "cashflow-bot",
  "version": "0.0.0",
  "description": "Cashflow bot",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "lint": "eslint src --fix --ext .ts",
    "test": "npm run lint && npm run type-check",
    "start": "babel-node src/index.ts --extensions \".ts\"",
    "dev": "nodemon",
    "type-check": "tsc --noEmit",
    "build": "rimraf dist && npm run build:js",
    "build:js": "babel src --out-dir dist --extensions \".ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noamokman/cashflow-bot.git"
  },
  "keywords": [],
  "author": "Noam Okman <noamokman@gmail.com> (https://github.com/noamokman)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noamokman/cashflow-bot/issues"
  },
  "homepage": "https://github.com/noamokman/cashflow-bot#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/node": "^7.12.17",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/lowdb": "^1.0.9",
    "@types/node": "^14.14.31",
    "@types/pino": "^6.3.6",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "eslint-config-noamokman": "^10.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-unicorn": "^28.0.2",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "env-var": "^7.0.0",
    "israeli-bank-scrapers": "^1.0.1",
    "lowdb": "^1.0.0",
    "pino": "^6.11.1",
    "telegraf": "^4.1.1",
    "telegraf-inline-menu": "6.2.0"
  }
}
